{
	"name": "@oskar-dragon/claude-code",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"typecheck": "bunx tsc --noEmit",
		"test": "bun test",
		"biome:check": "bunx biome check .",
		"biome:format": "bunx biome format --write .",
		"biome:lint": "bunx biome lint .",
		"biome:fix": "bunx biome check --write .",
		"biome:ci": "bunx biome ci .",
		"check:all": "bun run typecheck && bun run biome:ci && bun run test",
		"prepare": "husky"
	},
	"keywords": [
		"claude-code",
		"project-management",
		"cli"
	],
	"author": "Oskar Dragon",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.2.5",
		"@types/bun": "latest",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@stricli/core": "^1.2.0"
	}
}
